    BackgroundIndirect           :: 3;
    AllForegroundInitiallyHidden :: 4;
}

/// LibraryAppletExitReason
LibAppletExitReason :: enum u32 {
    Normal     :: 0;
    Canceled   :: 1;
    Abnormal   :: 2;
    Unexpected :: 10;
}

/// AppletApplicationExitReason
AppletApplicationExitReason :: enum u32 {
    Normal     :: 0;
