+sf_hdtr :: struct {
+    headers:  *iovec; /* pointer to an array of header struct iovec's */
+    hdr_cnt:  s32; /* number of header iovec's */
+    trailers: *iovec; /* pointer to an array of trailer struct iovec's */
+    trl_cnt:  s32; /* number of trailer iovec's */
