+*/
+tcphdr :: struct {
+    th_sport: u16; /* source port */
+    th_dport: u16; /* destination port */
+    th_seq:   tcp_seq; /* sequence number */
+    th_ack:   tcp_seq; /* acknowledgement number */
+
+    th_x2:    u8; /* (unused) */
+    #place th_x2; /*bitfield 4*/ th_off:   u8; /* data offset */
+
+    th_flags: u8;
+
+    th_win:   u16; /* window */
+    th_sum:   u16; /* checksum */
+    th_urp:   u16; /* urgent pointer */
+}
