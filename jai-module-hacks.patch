+*/
+omsghdr :: struct {
+    msg_name:         *u8; /* optional address */
+    msg_namelen:      s32; /* size of address */
+    msg_iov:          *iovec; /* scatter/gather array */
+    msg_iovlen:       s32; /* # elements in msg_iov */
+    msg_accrights:    *u8; /* access rights sent/received */
+    msg_accrightslen: s32;
